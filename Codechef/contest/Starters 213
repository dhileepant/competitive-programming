EXML Race

for t in range(int(input())):
    n=int(input())
    m=0
    # c = -1
    ans=1
    for i in range(1,n+1):
        d, t= map(int,input().split())
        # s = 
        s=d//t
        if s>m:
            m=s
            ans=i
    print(ans)

No 4 Please

for t in range(int(input())):
    n = int(input())
    l = list(map(int,input().split()))
    o = l.count(1)
    t = l.count(2)
    th = l.count(3)
    # ans = 0
    # ans += th 
    ans = max(0 , t-1)
    if o >= th:
        ans += th 
    else:
        ans += o 
    print(ans)

Separation

for t in range(int(input())):
    n, x = map(int,input().split())
    l = list(map(int,input().split()))
    mn = 0
    mx = 0
    # ans = -1
    e = 0
    for i in l:
        if i == x:
            e += 1  
        elif i > x:
            mx += 1 
        else:
            mn += 1 
    #  ans = True
    # if mx <= e or mn <= e:
    if e > 0 or mn == 0 or mx == 0:
    # if mx and mn:
        print('YeS')
    else:
        print('No')

