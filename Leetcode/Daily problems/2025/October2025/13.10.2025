class Solution:
    def removeAnagrams(self, words: List[str]) -> List[str]:
        n = len(words)
        i = 1
        while i < len(words):
            a = sorted(words[i])
            b = sorted(words[i-1])
            if a == b:
                words.remove(words[i])
                i -= 1
            i +=1
        return words
