    for i in range(1,n):
        if t[i]==c:
            z+=1
            x+=1
        else:
            b.append((c,z))
            c=t[i]
            z=1
    b.append((c,z))
    a=x
    l1,b1=b[0]
    r1,b2=b[-1]
    if l1==r1:
        print(a*k+(k-1)*b1)
    else:
        print(a*k)'''
    p = []
    c = []
    for i, ch in enumerate(s):
        if ch != 'I':
            p.append(i)
            c.append(ch)
    if not p:
        print(n * k - 1)
        continue
    m = len(p)
    a = p[0]
    b = n - 1 - p[-1]
    d = 0
    for i in range(m - 1):
        g = p[i + 1] - p[i] - 1
        if c[i] == c[i + 1]:
            d += p[i + 1] - p[i]
        else:
            d += g
    q = n - p[-1] + p[0]
    g = q - 1
    if c[-1] == c[0]:
        e = q
    else:
        e = g
    ans = a + b + k * d + (k - 1) * e
    print(ans)
    '''a = 0
