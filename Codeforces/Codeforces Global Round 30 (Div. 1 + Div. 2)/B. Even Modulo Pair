for t in range(int(input())):
    n=int(input())
    l=list(map(int,input().split()))
    f=0
    for i in range(n):
        for j in range(i+1,n):
            if ((l[j]%l[i])&1)==0:
                print(l[i],l[j])
                f=1
                break
        if f:
            break
    if not f:
        print(-1)
