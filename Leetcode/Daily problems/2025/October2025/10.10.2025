class Solution:
    def maximumEnergy(self, energy: List[int], k: int) -> int:
        ans = min(energy)
        n = len(energy)
        for i in range(n-k, n):
            res = 0
            for j in range(i, -1, -k):
                res += energy[j]
                ans = max(ans, res)
        return ans
