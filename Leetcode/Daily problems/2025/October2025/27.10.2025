class Solution:
    def numberOfBeams(self, bank: List[str]) -> int:
        ans = 0
        n = len(bank)
        for i in range(n):
            o = bank[i].count('1')
            if o == 0:
                continue
            for j in range(i+1,n):
                oo = bank[j].count('1')
                if oo == 0:
                    continue
                else:
                    r = o * oo
                    ans += r
                    break
        return ans
